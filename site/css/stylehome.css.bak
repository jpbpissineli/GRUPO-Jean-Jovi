html,body{
	margin:0;
	padding:0;
		font-family:"Helvetica",sans-seif;
		font-size: 14pt;
	}
	
body{
	overflow-x:hidden;
}

.header {
	background-color: #f7cc0a;
	position:relative;
	box-shadow:
	0px 3px 5px -1px rgba(0, 0, 0, 0.2),
	0px 6px 10px 0px rgba(0, 0, 0, 0.14),
	0px 1px 18px 0px rgba(0, 0, 0, 0.12);
}

.conteudo{
	display:flex;
}

ul, li {
	list-style: none;
	display: inline;
}

.icones {
	width: auto;
	height: 50px;
}

#icones {
	float: right;
}

#logo {
	margin: 0 auto;
    width: auto;
}